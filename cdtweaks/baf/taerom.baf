IF
  GlobalGT("MakeArmor","GLOBAL",1)
  PartyHasItem("%tutu_var%MISC12")
THEN
  RESPONSE #100
   SetGlobal("MakeArmor","GLOBAL",0)
   SetGlobalTimer("Taerom","GLOBAL",TEN_DAYS)
   Continue()
END

